{
  "build": {
    "cpu": "cortex-m4",
    "f_cpu": "120000000L",
    "mcu": "mk64fn1m0vdc12"
  },
  "debug": {
    "tools": {
      "cmsis-dap": {
        "server": {
          "package": "tool-openocd",
          "executable": "bin/openocd",
          "arguments": [
            "-f", "scripts/interface/cmsis-dap.cfg",
            "-c", "set CHIPNAME k64",
            "-f", "scripts/target/kx.cfg"
          ]
        },
        "gdbinit": [
          "target remote :3333",
          "file $PROG_PATH",
          "load $PROG_PATH",
          "monitor init",
          "monitor reset halt",
          "tb main"
        ]
      }
    }
  },
  "frameworks": [
    "mbed"
  ],
  "name": "Hexiwear",
  "upload": {
    "maximum_ram_size": 262144,
    "maximum_size": 1048576
  },
  "url": "https://developer.mbed.org/platforms/Hexiwear/",
  "vendor": "MikroElektronika"
}